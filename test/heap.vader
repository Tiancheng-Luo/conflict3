Execute(heap works):
  let h = conflict3#heap_empty()
  let h = conflict3#heap_insert(h, 2, "2")
  let h = conflict3#heap_insert(h, 1, "1")
  let h = conflict3#heap_insert(h, 4, "4")
  Assert !conflict3#heap_null(h)
  let [h, p, v] = conflict3#heap_pop(h)
  AssertEqual [1, "1"], [p, v]
  let h = conflict3#heap_insert(h, 3, "3")
  Assert !conflict3#heap_null(h)
  let [h, p, v] = conflict3#heap_pop(h)
  AssertEqual [2, "2"], [p, v]
  Assert !conflict3#heap_null(h)
  let [h, p, v] = conflict3#heap_pop(h)
  AssertEqual [3, "3"], [p, v]
  let h = conflict3#heap_insert(h, 0, "0")
  Assert !conflict3#heap_null(h)
  let [h, p, v] = conflict3#heap_pop(h)
  AssertEqual [0, "0"], [p, v]
  Assert !conflict3#heap_null(h)
  let [h, p, v] = conflict3#heap_pop(h)
  AssertEqual [4, "4"], [p, v]
  Assert conflict3#heap_null(h)
